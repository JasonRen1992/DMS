@{
    Layout = null;
}
<!DOCTYPE html>
<html>
    <head>
        <title>Index</title>
        <meta name="viewport" content="width=device-width" />
        @Scripts.Render("~/bundles/jquery")
        <script src="@Url.Content("~/Scripts/EasyUI/jquery.easyui.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/ViewScripts/Home/Index.js")" type="text/javascript"></script>
         @Styles.Render("~/Content/easyui/metro")
         @Styles.Render("~/Content/easyui/icon")
         @Styles.Render("~/Content/css")
    </head>
    <body class="easyui-layout">
        <div data-options="region:'north',border:false,split:false" style="height:60px;background:url(Content/Images/head_bg.gif) repeat-x left top;"></div>
        <div data-options="region:'west',split:true,iconCls:'icon-menu'" title="菜单列表" style="width: 200px; padding-top: 2px; background-color:#fff; overflow:auto">
            <div id="RightAccordion" class="easyui-accordion" ></div>
        </div>
        <div data-options="region:'center',border:false">
            <div id="mainTab" class="easyui-tabs" data-options="fit:true">
                <div title="首页" data-options="closable:true" style="overflow: hidden; background:#fff">
                    <iframe scrolling="auto" frameborder="0" src="http://www.cnblogs.com" style="width: 100%; height: 100%;"></iframe>
                </div>
            </div>
        </div>
        <div data-options="region:'south',border:false" style="background-color:#F2F5F7;text-align:center;padding:3px 0;">
            <div class="define-bottom">
                版权所有 系统管理 Copyright 2009-2014.jason.com. All Rights Reserved
            </div>
        </div>
    </body>
</html>